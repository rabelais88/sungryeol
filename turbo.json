{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "prebuild": {},
    "build": {
      "dependsOn": ["prebuild", "^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["prebuild", "build", "lint"],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "typescript-config/tsconfig.json",
    "prettier-config/index.js"
  ]
}
